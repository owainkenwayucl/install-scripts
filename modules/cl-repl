#%Module -*- tcl -*-
proc ModulesHelp { } {
  puts stderr "SBCL + opts to make it work"

}

module-whatis "SBCL + opts to make it work"

prereq gcc-libs
module load rlwrap/0.43
module load sbcl/1.3.19

set-alias cl-repl "rlwrap sbcl --noinform --eval '(setf *read-default-float-format* \'double-float)'"
