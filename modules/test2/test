#%Module -*- tcl -*-
##

proc ModulesHelp { } {

  puts stderr {Test module}

}

module-whatis {Test module}

prereq gcc-libs/4.9.2

conflict test2

set test_var 3.5

if { [info exists ::env(PYTHON_VERSION_TAG)] } {
   set test_var $::env(PYTHON_VERSION_TAG)
}

if { $test_var=="3.5" } {prereq python/3.5.2}
if { $test_var=="3.6" } {prereq python/3.6.3}

setenv TEST_MODULE $test_var


